<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.ev.jonathan.emprendeelvieajeev.AcercaDe">



    <TextView
        android:id="@+id/tv_acercade_titulo"
        android:layout_width="match_parent"
        android:layout_height="76dp"
        android:gravity="center"
        android:text="Accerca de Emprende el Viaje EV"
        android:textAlignment="center"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="140dp"
        tools:layout_editor_absoluteY="16dp" />

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:context="com.ev.jonathan.ev_emprendeelviaje.LugarTuristico">

        <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            tools:context="com.ev.jonathan.ev_emprendeelviaje.LugarTuristico">

            <TextView
                android:id="@+id/tv_acercade"
                android:layout_width="match_parent"
                android:layout_height="49dp"
                android:gravity="center"
                android:text="Emprende el viaje 0.3"
                android:textStyle="italic"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="140dp"
                tools:layout_editor_absoluteY="16dp" />

            <TextView
                android:id="@+id/tv_acercade_info"
                android:layout_width="match_parent"
                android:layout_height="58dp"
                android:gravity="center"
                android:text="Aplicacion movil que ayuda a encontrar \ntodos los lujares turisticos de tu alrededor"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="140dp"
                tools:layout_editor_absoluteY="16dp" />

            <TextView
                android:id="@+id/tv_acercade_copyright"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="Escuela PolitÃ©cnica Nacional\nCopyright 2018 "
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="140dp"
                tools:layout_editor_absoluteY="16dp" />

            <TextView
                android:id="@+id/tv_acercade_creadopor"
                android:layout_width="match_parent"
                android:layout_height="37dp"
                android:gravity="center"
                android:text="Creado por:"
                android:textStyle="bold"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="140dp"
                tools:layout_editor_absoluteY="16dp" />

            <TextView
                android:id="@+id/tv_acercade_creador1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="Jonathan Caiza"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="140dp"
                tools:layout_editor_absoluteY="16dp" />

            <TextView
                android:id="@+id/tv_acercade_creador2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="Samantha de la Cruz"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="140dp"
                tools:layout_editor_absoluteY="16dp" />

            <TextView
                android:id="@+id/tv_acercade_creador3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="Sonia Jaguaco"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="140dp"
                tools:layout_editor_absoluteY="16dp" />

            <TextView
                android:id="@+id/tv_acercade_creador4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="Cynthia Oyana"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="140dp"
                tools:layout_editor_absoluteY="16dp" />
        </LinearLayout>

    </LinearLayout>

</LinearLayout>
